{
  "name": "tangrams",
  "version": "0.11.0",
  "description": "Code generation for the TanStack ecosystem",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tangrams": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "tanstack",
    "react-query",
    "graphql",
    "openapi",
    "swagger",
    "codegen",
    "typescript",
    "code-generation"
  ],
  "author": "hobbescodes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hobbescodes/tangrams.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/hobbescodes/tangrams#readme",
  "bugs": {
    "url": "https://github.com/hobbescodes/tangrams/issues"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "c12": "^3.3.2",
    "chokidar": "^5.0.0",
    "citty": "^0.1.6",
    "code-block-writer": "^13.0.3",
    "consola": "^3.4.0",
    "fast-glob": "^3.3.3",
    "graphql": "^16.10.0",
    "micromatch": "^4.0.8",
    "openapi-types": "^12.1.3",
    "picomatch": "^4.0.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.80.6",
    "@types/bun": "latest",
    "@types/micromatch": "^4.0.9",
    "@types/picomatch": "^4.0.2",
    "@vitest/coverage-istanbul": "^3.2.3",
    "arktype": "^2.1.20",
    "effect": "^3.12.0",
    "graphql-request": "^7.1.2",
    "tsup": "^8.5.0",
    "valibot": "^1.1.0",
    "vitest": "^3.2.3",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "@better-fetch/fetch": ">=1.0.0",
    "@tanstack/react-query": ">=5.0.0",
    "arktype": ">=2.0.0",
    "effect": ">=3.0.0",
    "graphql-request": ">=6.0.0",
    "valibot": ">=1.0.0",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@better-fetch/fetch": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "arktype": {
      "optional": true
    },
    "effect": {
      "optional": true
    },
    "graphql-request": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
