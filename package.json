{
  "name": "tangen",
  "version": "0.6.0",
  "description": "Code generation for the TanStack ecosystem",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tangen": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "keywords": [
    "tanstack",
    "react-query",
    "graphql",
    "codegen",
    "typescript",
    "code-generation"
  ],
  "author": "hobbescodes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hobbescodes/tangen.git"
  },
  "homepage": "https://github.com/hobbescodes/tangen#readme",
  "bugs": {
    "url": "https://github.com/hobbescodes/tangen/issues"
  },
  "dependencies": {
    "c12": "^3.3.2",
    "chokidar": "^5.0.0",
    "citty": "^0.1.6",
    "consola": "^3.4.0",
    "fast-glob": "^3.3.3",
    "graphql": "^16.10.0",
    "picomatch": "^4.0.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@changesets/cli": "^2.29.8",
    "@tanstack/react-query": "^5.80.6",
    "@types/bun": "latest",
    "@types/picomatch": "^4.0.2",
    "@vitest/coverage-istanbul": "^3.2.3",
    "graphql-request": "^7.1.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "graphql-request": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "graphql-request": {
      "optional": true
    }
  }
}
